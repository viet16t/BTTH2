using System.Data;
using System.Runtime.CompilerServices;
using System.Net.Http;
using System;
@model IEnumerable<DoanQuocVietBTTH2.Models.Student>
<p>
   <a class="btn btn-success" asp-action="Create">Create a new Student</a>
</p>
<table class="table table-all"
    <tr>
       <th>Student ID </th>
       <th>Student Name </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
           <td>@Html.DisplayFor(m => item.StudentID)</td>
           <td>@Html.DisplayFor(m => item.StudentName)</td>
           <td>
            <a asp-action="Edit" asp-route-id="@item.StudentID">Sửa</a>  |
            <a asp-action= "Delete" asp- route-id=@item.StudentName> Xóa </a>
           </td>
        </tr>
    }
</table>